{
  "name" : "root-folder",
  "scripts": {
    "install": "npm-run-all --parallel server:install route-recommender-app:install",
    "start": "npm-run-all --parallel server:start route-recommender-app:start",
    "server:install": "cd server && npm install",
    "server:start": "cd server && npm start",
    "route-recommender-app:install": "cd route-recommender-app && npm install",
    "route-recommender-app:start": "cd route-recommender-app && npm start"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "@react-google-maps/api": "^2.18.1",
    "react-router-dom": "^6.10.0",
    "swiper": "^9.2.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
